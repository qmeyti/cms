{
    "name": "qmeyti/menubuilder",
    "description": "Drag and drop menu generator",
    "keywords": ["Laravel", "Menu"],
    "authors": [
        {
            "name": "Mehdi hajatpour",
            "email": "mehdi.hajatpour@gmail.com",
            "role" : "Original Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "8.*"
    },
    "autoload": {
        "psr-4": {
          "SoeurngSar\\MenuBuilder\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoeurngSar\\MenuBuilder\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "SoeurngSar\\MenuBuilder\\app\\Facades\\Menu"
            }
        }
    }
}
